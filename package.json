{
  "name": "@goblin-stone/website",
  "version": "0.0.1",
  "description": "Goblin Stone Web Server",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "test": "jest",
    "clean": "rimraf public && rimraf build",
    "dev": "npm run clean && npm-run-all --parallel dev:build dev:start",
    "dev:build": "cross-env NODE_ENV=development webpack --watch --mode development",
    "dev:start": "cross-env NODE_ENV=development browser-refresh --inspect server/server.js"
  },
  "sideEffects": [
    "/clients/client.js",
    "/server/server.js",
    "/server/middlewares/*",
    "*.(css|scss)",
    "*.(png|jpg|gif)"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.6",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redis": "^3.0.2",
    "serialize-javascript": "^3.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@svgr/webpack": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "browser-refresh": "^1.7.3",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-loader": "^0.5.5",
    "http-proxy-middleware": "^1.0.4",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "zlib": "^1.0.5"
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 version"
    ]
  }
